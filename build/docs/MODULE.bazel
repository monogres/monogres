module(
    name = "docs",
    bazel_compatibility = [">=7.1.0"],
)

bazel_dep(name = "monogres", version = "", dev_dependency = True)
bazel_dep(name = "aspect_bazel_lib", version = "2.20.0", dev_dependency = True)
